{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/test/onesql.test.ts",
			"outFiles": [ "${workspaceRoot}/../out/lang/**/*.js" ],
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "internalConsole",
			"sourceMaps": true
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"sourceMaps": true,
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		}
	]
}